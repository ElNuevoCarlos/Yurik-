<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <% guilds_bot.map(guild => { %>
    <% if (guild.icon) { iconurl="https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon
          + ".png??size=512" ; } else {
          iconurl="https://blog.logomyway.com/wp-content/uploads/2020/12/discord-mascot.png" ; } %>
        <div>
          <div>
            <img src="<%= iconurl %>" />
          </div>
        <div>
          <%= guild.name %>
        </div>
        <div>
          <a href="/dashboard/<%= guild.id %>">
            <button>Manage</button>
          </a>
        </div>
      </div>
  <% })%>
  <% guilds_no_bot.map(guild => { %>
    <% if (guild.icon) { iconurl="https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon
          + ".png??size=512" ; } else {
          iconurl="https://blog.logomyway.com/wp-content/uploads/2020/12/discord-mascot.png" ; } %>
    <div>
      <div>
        <img src="<%= iconurl %>" />
      </div>
      <div>
        <%= guild.name %>
      </div>
      <div>
        <a href="">
          <button>Invite</button>
        </a>
      </div>
    </div>
  <% })%>
</body>

</html>